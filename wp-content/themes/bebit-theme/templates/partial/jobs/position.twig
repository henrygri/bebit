      <div class="card-body">
      	<div class="container">
	      	<div class="px-4">
				{{ job.descrizione_posizione }}
				<div class="row mt-5">
					{% if job.livello_posizione|length %}
					<div class="col-lg-3 mb-4">
						<p>Livello di anzianit√†</p>
						<strong>{{ job.livello_posizione }}</strong>
					</div>
					{% endif %}
					{% if job.tipo_di_impiego|length %}
					<div class="col-lg-3 mb-4">
						<p>Tipo di impiego</p>
						<strong>{{ job.tipo_di_impiego }}</strong>
					</div>
					{% endif %}
					{% if job.funzione_lavorativa|length %}
					<div class="col-lg-3 mb-4">
						<p>Funzione lavorativa</p>
						<strong>{{ job.funzione_lavorativa }}</strong>
					</div>
					{% endif %}
					{% if job.settori_posizione|length %}
					<div class="col-lg-3 mb-4">
						<p>Settori</p>
						<strong>{{ job.settori_posizione }}</strong>
					</div>
					{% endif %}
				</div>
	      	</div>
      	</div>
        <div class="form-section mt-5">
			<h3 class="text-center">candidati</h3>
			<div class="py-5 mt-5" style="background-color:#FBFBFB">
				<div class="container">
					{{ function('do_shortcode', '[contact-form-7 id="103" title="Form jobs"]') }}
				</div>
			</div>
      </div>
