{% if menu %}
	<div class="menu-navbar">
		<div class="container relative">
			<ul>
			{% for item in items %}
				<li class="{{ item.classes | join(' ') }}">
					<a target="{{ item.target }}" href="{{ item.link }}" title="{{ item.title }}">{{ item.title }}</a>
					{% if item.children %}
						<ul class="item-children-list">
							{% for item in item.children %}
								<li><a href="{{ item.link }}" title="{{ item.title }}">{{item.title}}</a></li>
							{% endfor %}
						</ul>
					{% endif %}
				</li>
			{% endfor %}
			</ul>
			<div class="icl_selector_languages">
				<ul>
					{{ function('icl_selector_languages') }}
				</ul>
			</div>
		</div>
	</div>
{% endif %}
